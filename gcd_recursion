def gcd(a,b):     #A recursive function to compute gcd of two numbers
    if(b==0):
        return a
    else :
        return gcd(b,a%b)   # returning gcd 
a=int(input(" enter an integer "))
b=int(input(" enter another integer "))

print(" the gcd is " , gcd(a,b))    # printing gcd of the two numbers taken as input
        
